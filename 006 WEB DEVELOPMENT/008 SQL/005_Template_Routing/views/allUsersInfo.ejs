<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Information</title>
    <style>
        td,th,tr,table {
            border: 2px solid;
            border-collapse: collapse;
        }
    </style>
</head>

<body>
    <div>
        <table>
            <tr>
                <th> SL.No </th>
                <th> Name </th>
                <th> Age </th>
                <th> Department </th>
            </tr>
            <% for(let user of results){ %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.name %></td>
                    <td><%= user.age%></td>
                    <td><%= user.department%></td>
                </tr>
            <% } %>
        </table>
    </div>
</body>

</html>